{
    "name": "moistcake/db-backup",
    "description": "Database backup to Google Drive for Laravel projects.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Muiz Shukri",
            "email": "muizshukri06@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0",
        "spatie/db-dumper": "^3.0",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Moistcake\\DbBackup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moistcake\\DbBackup\\DbBackupServiceProvider"
            ]
        }
    }
}
